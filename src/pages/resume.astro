---
import Layout from "../layouts/MacLayout.astro";
---

<Layout title="ethanlew.dev - Resumé">
	<div class="flex flex-col gap-4">
		<div>
			<h1>Resumé</h1>
			<a class="btn text-xl" href="/">
				<i class="i-fa6-solid:arrow-left"></i>
				Back to Index Card
			</a>
		</div>

		<div class="border-2 rounded-md">
			<iframe
				title="Resumé"
				src="https://drive.google.com/file/d/1UnzLa1QXqxL_RCRCOSA0OtZpnqF3MZAM/preview"
				class="h-80 w-full"
				allow="autoplay"></iframe>
		</div>

		<a
			href="https://drive.google.com/file/d/1UnzLa1QXqxL_RCRCOSA0OtZpnqF3MZAM/view?usp=sharing"
			class="btn"
			target="_blank"
			rel="noopener noreferrer">Download</a
		>
	</div>
</Layout>
